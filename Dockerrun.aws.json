{
"AWSEBDockerrunVersion": 2,
"containerDefinitions":[
    {
        "name": "client",
        "image": "adeshadk/multi-client",
        "hostname": "client",
        "essesntial": false,
        "memory": 128
    },
    {
        "name": "server",
        "image": "adeshadk/multi-server",
        "hostname": "api",
        "essesntial": false,
        "memory": 128
    },
    {
        "name": "worker",
        "image": "adeshadk/multi-worker",
        "hostname": "worker",
        "essesntial": false,
        "memory": 128
    },
    {
        "name": "nginx",
        "image": "adeshadk/multi-nginx",
        "hostname": "nginx",
        "essesntial": true,
        "portMappings":[
            {
            "hostPort": 80,
            "containerPort": 80
            }
            ]  ,
        "links": ["client","server"],
        "memory": 128

    }
  ]
}